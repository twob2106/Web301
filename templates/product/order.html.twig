 {% extends 'base.html.twig' %}
 
 
{% block body %}
<body>
    <h1>Order List</h1>
    <table>
        <thead>
            <tr>
                <th>Order ID</th>
                <th>Product</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            {% for orderMap in orderMaps %}
                <tr>
                    <td>{{ orderMap.order.id }}</td>
                    <td>{{ orderMap.product.name }}</td>
                    <td>{{ orderMap.quantity }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
</body>




{% endblock %}